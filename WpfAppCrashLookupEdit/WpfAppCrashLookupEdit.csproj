<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <RootNamespace>WpfAppCrashLookupEdit</RootNamespace>
    <AssemblyName>WpfAppCrashLookupEdit</AssemblyName>
    <TargetFramework>net48</TargetFramework>
